<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">

      <em:id>macdict@oyenamit</em:id>
      <em:name>MacDict</em:name>
      <em:description>Lookup selected text in the built-in Mac OS X Dictionary.</em:description>
      <em:version>0.9.1</em:version>
      <em:creator>oyenamit</em:creator>
      <em:iconURL>chrome://macdict/skin/macdict-iconURL.png</em:iconURL>
      <em:icon64URL>chrome://macdict/skin/macdict-icon64URL.png</em:icon64URL>
      <!-- TODO: homepageURL -->
      <em:type>2</em:type>
      <em:targetPlatform>Darwin</em:targetPlatform>

      <!-- Make the addon compatible with Firefox -->
      <em:targetApplication>
          <Description>
              <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
              <em:minVersion>4.0</em:minVersion>
              <em:maxVersion>29.0</em:maxVersion>
          </Description>
      </em:targetApplication>

      <!-- German localization -->
      <em:localized>
          <Description>
              <em:locale>de-DE</em:locale>
              <em:name>MacDict</em:name>
              <em:description>Ausgew√§hlten Text im OS X Lexikon nachschlagen.</em:description>
              <em:creator>oyenamit</em:creator>
              <em:translator>Sierk Bornemann</em:translator>
          </Description>
      </em:localized>

  </Description>
</RDF>

